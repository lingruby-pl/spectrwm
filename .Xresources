!! [options]

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

XTerm*renderFont: true
XTerm*faceName: xft:Hack
XTerm*faceSize: 9.5
XTerm*utf8: 2
XTerm*locale: true


!! [colors]
*background:  #121212
*foreground:  #c5c8c6

! black
*color0:  #292d3e
*color8:  #434758

! red
*color1:  #a54242
*color9:  #ff8b92

! green
*color2:  #c3e88d
*color10:  #ddffa7

! yellow
*color3:  #c59d00
*color11:  #ffe585

! blue
*color4:  #1378ab
*color12:  #9cc4ff

! magenta
*color5:  #c792ea
*color13:  #e1acff

! cyan
*color6:  #89ddff
*color14:  #a3f7ff

! white
*color7:  #d0d0d0
*color15:  #ffffff



!! Transparency (0-1):
st.alpha: 0.8

!! Set a default font and font size as below:
st.font: Hack-9.5;

! st.termname: st-256color
! st.borderpx: 2

!! Set the background, foreground and cursor colors as below:
*.background: #121212
*.foreground: #aaaaaa
*.cursorColor: #aaaaaa

