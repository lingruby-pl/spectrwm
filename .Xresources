!! Rxvt-unicode
URxvt.termName: rxvt-unicode-256color
URxvt*imLocale: pl_PL.UTF-8
URxvt.font:                xft:Hack:size=9.5
URxvt.boldFont:            xft:Hack:size=9.5:bold
URxvt.italicFont:          xft:Hack:size=9.5:italic
URxvt.boldItalicFont:      xft:Hack:size=9.5:bold:italic
URxvt.letterSpace:         -1
URxvt.saveLines:           1000000
URxvt*color0:              #292d3e
URxvt*color1:              #a54242
URxvt*color2:              #c3e88d
URxvt*color3:              #c59d00
URxvt*color4:              #317CA5
URxvt*color5:              #c792ea
URxvt*color6:              #89ddff
URxvt*color7:              #d0d0d0
URxvt*color8:              #434758
URxvt*color9:              #ff8b92
URxvt*color10:             #ddffa7
URxvt*color11:             #ffe585
URxvt*color12:             #9cc4ff
URxvt*color13:             #e1acff
URxvt*color14:             #a3f7ff
URxvt*color15:             #ffffff
URxvt*background:          #121212
URxvt*foreground:          #aaaaaa
URxvt*cursorColor:         #aaaaaa
URxvt.internalBorder:      0
URxvt.externalBorder:      0
URxvt.scrollBar:           false
URxvt*fading:              30
URxvt.perl-ext-common:     default,matcher,clipboard
URxvt.url-launcher:        vivaldi-snapshot
URxvt.matcher.button:      1
URxvt.colorUL:             #317CA5
URxvt*scrollBar:           false
URxvt*scrollBar_right:     false
URxvt*transparent:         false
URxvt*depth:               32
URxvt.background:          [80]#121212
*antialias:  true
*autohint:   true
URxvt.clipboard.autocopy: true
URxvt.keysym.C-c: perl:clipboard:copy
URxvt.keysym.C-v: perl:clipboard:paste
Rxvt.iso14755                 : false
URxvt.iso14755_52             : false
