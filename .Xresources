!! [options]

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

!! Rxvt-unicode
Xcursor*theme:             LinGruby-drop
Xcursor*size:              0
URxvt*termName:            rxvt-unicode-256color
URxvt*buffered:            false
URxvt*intensityStyles:     false
URxvt*imLocale:            pl_PL.UTF-8
URxvt*font:                xft:Hack:size=9.5, \
                           xft:JoyPixels:size=10, \
                           xft:Noto Color Emoji:size=10, \
                           xft:Symbola:size=10:minspace=False
URxvt*boldFont:            xft:Hack:size=9.5:bold
URxvt*italicFont:          xft:Hack:size=9.5:italic
URxvt*boldItalicFont:      xft:Hack:size=9.5:bold:italic
URxvt*letterSpace:         -1
URxvt*lineSpace:           -1
URxvt*saveLines:           1000000

! special
URxvt*foreground:          #a9b1d6
URxvt*background:          [80]#1a1b26
URxvt*cursorColor:         #a9b1d6
urxvt*colorBD:             #1378ab

! black
URxvt*color0:              #1a1b26
URxvt*color8:              #4e5173

! red
URxvt*color1:              #F7768E
URxvt*color9:              #F7768E

! green
URxvt*color2:              #9ECE6A
URxvt*color10:             #9ECE6A

! yellow
URxvt*color3:              #E0AF68
URxvt*color11:             #E0AF68

! blue
URxvt*color4:              #7AA2F7
URxvt*color12:             #7AA2F7

! magenta
URxvt*color5:              #9a7ecc
URxvt*color13:             #9a7ecc

! cyan
URxvt*color6:              #4abaaf
URxvt*color14:             #4abaaf

! white
URxvt*color7:              #acb0d0
URxvt*color15:             #acb0d0

URxvt*cursorUnderline:     true
URxvt*cursorBlink:         true
URxvt*internalBorder:      0
URxvt*externalBorder:      0
URxvt*scrollBar:           false
URxvt*loginShell:          true
URxvt*fading:              0
URxvt*perl-ext-common:     default,matcher,clipboard
URxvt*url-launcher:        vivaldi-snapshot
URxvt*matcher.button:      1
URxvt*colorUL:             #0087bd
URxvt.scrollBar:           false
URxvt*scrollBar_right:     false
URxvt*transparent:         false
URxvt*depth:               32
*antialias:                true
*autohint:                 true
URxvt*clipboard.autocopy:  true
URxvt*keysym.C-c:          perl:clipboard:copy
URxvt*keysym.C-v:          perl:clipboard:paste
Rxvt*iso14755:             false
URxvt*iso14755_52:         false
URxvt*skipBuiltinGlyphs:   true


!! XTerm:UXterm
XTerm*renderFont: true
XTerm*faceName: xft:Hack
XTerm*faceSize: 9.5
XTerm*utf8: 2
XTerm*locale: true
XTerm*loginShell: true

!! [colors]
! special
*foreground:   #a9b1d6
*background:   #1a1b26
*cursorColor:  #a9b1d6

! black
*color0:              #1a1b26
*color8:              #4e5173

! red
*color1:              #F7768E
*color9:              #F7768E

! green
*color2:              #9ECE6A
*color10:             #9ECE6A

! yellow
*color3:              #E0AF68
*color11:             #E0AF68

! blue
*color4:              #7AA2F7
*color12:             #7AA2F7

! magenta
*color5:              #9a7ecc
*color13:             #9a7ecc

! cyan
*color6:              #4abaaf
*color14:             #4abaaf

! white
*color7:              #acb0d0
*color15:             #acb0d0


!! Transparency (0-1):
st.alpha: 0.8

!! Set a default font and font size as below:
st.font: Hack-9.5;

! st.termname: st-256color
! st.borderpx: 2

!! Set the background, foreground and cursor colors as below:
*.background: #1a1b26
*.foreground: #a9b1d6
*.cursorColor: #a9b1d6

