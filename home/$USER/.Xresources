!! [options]
! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------

!! Xft.dpi: 96
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter
Xft.antialias: 1
Xft.hinting: 1
Xft.rgba: rgb

! ------------------------------------------------------------------------------
! URxvt Fonts
! ------------------------------------------------------------------------------

URxvt*font:                xft:JetBrains Mono:bold:size=9.5:antialias=true:hinting=true, \
                           xft:Noto Emoji:pixelsize=6:minspace=False:antialias=false, \
                           xft:Symbola:pixelsize=6:minspace=False:antialias=False
URxvt*boldFont:            xft:JetBrains Mono:bold:size=9.5:antialias=true:hinting=true
URxvt*italicFont:          xft:JetBrains Mono:italic:size=9.5:antialias=true:hinting=true
URxvt*boldItalicFont:      xft:JetBrains Mono:bold:italic:size=9.5:antialias=true:hinting=true

! ------------------------------------------------------------------------------
! URxvt configs
! ------------------------------------------------------------------------------

!! Rxvt-unicode
Xcursor*theme:             LinGruby-drop
Xcursor*size:              0
URxvt*termName:            rxvt-unicode-256color
URxvt*buffered:            false
URxvt*intensityStyles:     false
URxvt*imLocale:            pl_PL.UTF-8
URxvt*geometry:            125x45
URxvt*letterSpace:         -1
URxvt*lineSpace:           -1
URxvt*saveLines:           15000
URxvt*cursorUnderline:     true
URxvt*cursorBlink:         true
URxvt*internalBorder:      2
URxvt*externalBorder:      2
URxvt*scrollBar:           false
URxvt*loginShell:          true
URxvt*fading:              0
URxvt*perl-ext-common:     default,matcher,clipboard
URxvt*url-launcher:        vivaldi-snapshot
URxvt*matcher.button:      1
URxvt*colorUL:             #5F819D
URxvt.underlineURLs:       true
URxvt.scrollBar:           false
URxvt*scrollBar_right:     false
URxvt*transparent:         false
URxvt*depth:               32
*antialias:                true
*autohint:                 true
URxvt*clipboard.autocopy:  true
URxvt*keysym.S-C-C:        perl:clipboard:copy
URxvt*keysym.S-C-V:        perl:clipboard:paste
Rxvt*iso14755:             false
URxvt*iso14755_52:         false
URxvt*skipBuiltinGlyphs:   true

! ------------------------------------------------------------------------------
! URxvt Colors
! ------------------------------------------------------------------------------

! special
URxvt*foreground:          #c29f6f
URxvt*background:          [80]#080808
URxvt*cursorColor:         #49a5bc
URxvt*colorBD:             #447999

! black
URxvt*color0:       #292929
URxvt*color8:       #494949

! red
URxvt*color1:       #854340
URxvt*color9:       #854340

! green
URxvt*color2:       #97a293
URxvt*color10:      #97a293

! yellow
URxvt*color3:       #c29f6f
URxvt*color11:      #c29f6f

! blue
URxvt*color4:       #5e606c
URxvt*color12:      #5e606c

! magenta
URxvt*color5:       #7a6054
URxvt*color13:      #7a6054

! cyan
URxvt*color6:       #78746c
URxvt*color14:      #78746c

! white
URxvt*color7:       #bababa
URxvt*color15:      #bababa

! ------------------------------------------------------------------------------
! Rofi configs
! ------------------------------------------------------------------------------

! Rofi
rofi.font: JetBrains Mono 9.5

! ------------------------------------------------------------------------------
! XTerm:UXterm configs
! ------------------------------------------------------------------------------

!! XTerm:UXterm
XTerm*renderFont: true
XTerm*faceName: xft:JetBrains Mono
XTerm*faceSize: 9.5
XTerm*utf8: 2
XTerm*locale: true
XTerm*loginShell: true

! ------------------------------------------------------------------------------
! Colors XTerm:UXterm
! ------------------------------------------------------------------------------

! special
XTerm*background:   #080808
XTerm*foreground:   #c29f6f
XTerm*cursorColor:  #49a5bc

! black
XTerm*color0:       #292929
XTerm*color8:       #494949

! red
XTerm*color1:       #854340
XTerm*color9:       #854340

! green
XTerm*color2:       #97a293
XTerm*color10:      #97a293

! yellow
XTerm*color3:       #c29f6f
XTerm*color11:      #c29f6f

! blue
XTerm*color4:       #5e606c
XTerm*color12:      #5e606c

! magenta
XTerm*color5:       #7a6054
XTerm*color13:      #7a6054

! cyan
XTerm*color6:       #78746c
XTerm*color14:      #78746c

! white
XTerm*color7:       #bababa
XTerm*color15:      #bababa

! ------------------------------------------------------------------------------
! st configs
! ------------------------------------------------------------------------------

!! Transparency (0-1):
st.alpha: 0.85

!! Set a default font and font size as below:
st.font: JetBrains Mono:extrabold:pixelsize=11.5:antialias=true:hinting=true

! st.termname: st-256color
! st.borderpx: 2

!! Set the background, foreground and cursor colors as below:

! ------------------------------------------------------------------------------
! st Colors
! ------------------------------------------------------------------------------

! special
st*background:   #080808
st*foreground:   #c29f6f
st*cursorColor:  #49a5bc

!! st-night:

! black
st*color0:       #292929
st*color8:       #494949

! red
st*color1:       #854340
st*color9:       #854340

! green
st*color2:       #97a293
st*color10:      #97a293

! yellow
st*color3:       #c29f6f
st*color11:      #c29f6f

! blue
st*color4:       #5e606c
st*color12:      #5e606c

! magenta
st*color5:       #7a6054
st*color13:      #7a6054

! cyan
st*color6:       #78746c
st*color14:      #78746c

! white
st*color7:       #bababa
st*color15:      #bababa

